<odoo>
    <record id="action_rrs_dash_register_status" model="ir.actions.act_window">
        <field name="name">Status</field>
        <field name="res_model">rrs.register.status</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="rrs_register_status_tree_view" model="ir.ui.view">
        <field name="name">Status</field>
        <field name="model">rrs.register.status</field>
        <field name="arch" type="xml">
            <tree string="status">
                <field name="name"/>
            </tree>
        </field>
    </record>
    
    <record id="rrs_register_form_view" model="ir.ui.view">
        <field name="name">rrs.register.status.form</field>
        <field name="model">rrs.register.status</field>
        <field name="arch" type="xml">
            <form string="Status">
            <sheet>
                <field name="name"/>
            </sheet>
            </form>
    </field>
    </record>
</odoo>